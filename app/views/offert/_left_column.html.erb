
<div name="models">abc</div>

<% for company in @companies %>
  <%= link_to("", onclick: "modelsVisibility") do %>
    <div class="left_link_main">
      <%= company_name(company) %>
    </div>   
  <% end %>

  <!-- HERE I ADD MODELS -->

  <% for model in company.models %>
    <%= link_to("") do %>
      <div class="left_link_second" name="models">
        <%= model.name %>
      </div>

      <!-- HERE I ADD VERSIONS -->

      <% for version in model.versions %>
        <%= link_to("") do %>

          <div class="left_link_thirth" name="versions">
            <%= version.name %>
          </div>
        <% end %>

      <% end %>
    <% end %>
  <% end %>
<% end %>

